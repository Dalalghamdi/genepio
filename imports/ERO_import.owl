    <!-- http://purl.obolibrary.org/obo/ERO_0000639 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/ERO_0000639">
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">cycle sequencing</rdfs:label>
