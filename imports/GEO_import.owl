        <obo:IAO_0000119 xml:lang="en">http://en.wikipedia.org/wiki/Sovereign_state</obo:IAO_0000119>
        <obo:IAO_0000412 rdf:resource="http://purl.obolibrary.org/obo/geo.owl"/>
