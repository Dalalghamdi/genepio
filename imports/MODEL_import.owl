                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onDataRange rdf:resource="http://www.w3.org/2001/XMLSchema#int"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <IFM:regExValidate rdf:datatype="http://www.w3.org/2001/XMLSchema#string">^([-+]?[1-9]\d*|0)$</IFM:regExValidate>
        <rdfs:label xml:lang="en">integer</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/IRIDA/IFM#IFM_0000019 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/IRIDA/IFM#IFM_0000019">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/IRIDA/IFM#IFM_0001478"/>
        <owl:disjointWith rdf:resource="http://purl.obolibrary.org/obo/IRIDA/IFM#IFM_0000025"/>
        <IFM:regExDisplayFormat xml:lang="en">/^(\d(\.\d+)?)$/, &apos;+\1&apos;</IFM:regExDisplayFormat>
        <IFM:regExNormalize xml:lang="en">/^[N+](:&lt;lat&gt;\d(\.\d+)?)$/, $lat
/^S(:&lt;lat&gt;\d(\.\d+)?)$/,  &apos;-&apos; + $lat</IFM:regExNormalize>
        <IFM:regExValidate rdf:datatype="http://www.w3.org/2001/XMLSchema#string">/^(:&lt;lat&gt;[-S+N])?(:&lt;degree&gt;[1-8]?[0-9](:&lt;decimal&gt;\.\d{1,6})?)$/, $lat$degree.$decimal
/^(:&lt;lat&gt;[-S+N])?(:&lt;degree&gt;90(:&lt;decimal&gt;\.0{1,6})?))$/, $lat$degree.$decimal</IFM:regExValidate>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">See http://www.csgnetwork.com/gpscoordconv.html

See http://www.geomidpoint.com/latlon.html
See http://www.ontobee.org/browser/rdf.php?o=OBI&amp;iri=http://purl.obolibrary.org/obo/OBI_0001621
See http://www.w3.org/2003/01/geo/wgs84_pos</rdfs:comment>
        <rdfs:label xml:lang="en">latitude</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/IRIDA/IFM#IFM_0000020 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/IRIDA/IFM#IFM_0000020">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/IRIDA/IFM#IFM_0001559"/>
        <IFM:regExDisplayFormat rdf:datatype="http://www.w3.org/2001/XMLSchema#string">($area) $central-$station</IFM:regExDisplayFormat>
        <IFM:regExValidate rdf:datatype="http://www.w3.org/2001/XMLSchema#string">/^(:&lt;area&gt;(\([0-9]{3})\)|[0-9]{3})(:&lt;sep&gt;[- ])?(:&lt;central&gt;\d{3})\2(:&lt;station&gt;\d{4})$/, $area $central $station</IFM:regExValidate>
        <rdfs:label xml:lang="en">phone number (North America)</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/IRIDA/IFM#IFM_0000024 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/IRIDA/IFM#IFM_0000024">
