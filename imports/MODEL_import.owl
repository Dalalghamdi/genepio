    <!-- http://purl.obolibrary.org/obo/IRIDA/IFM#IFM_0000014 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/IRIDA/IFM#IFM_0000014">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/IRIDA/IFM#IFM_0001482"/>
        <IFM:regExDisplayFormat rdf:datatype="http://www.w3.org/2001/XMLSchema#string">$year/$month/$day</IFM:regExDisplayFormat>
        <IFM:regExNormalize rdf:datatype="http://www.w3.org/2001/XMLSchema#string">$year/$month/$day</IFM:regExNormalize>
        <IFM:regExValidate rdf:datatype="http://www.w3.org/2000/01/rdf-schema#Literal">/^(:&lt;year&gt;(19|20)\d\d)([- /.])(:&lt;month&gt;0[1-9]|1[012])\2(:&lt;day&gt;0[1-9]|[12][0-9]|3[01])$/
/^(:&lt;month&gt;0[1-9]|1[012])([- /.])(:&lt;day&gt;0[1-9]|[12][0-9]|3[01])\2(:&lt;year&gt;(19|20)\d\d)$/ 
/^(:&lt;day&gt;0[1-9]|[12][0-9]|3[01])([- /.])(:&lt;month&gt;0[1-9]|1[012])\2(:&lt;year&gt;(19|20)\d\d)$/</IFM:regExValidate>
        <rdfs:comment xml:lang="en">The instant is subject to the level of granularity of the observed time.  Year / Month / Day / Hour / Minute / Second</rdfs:comment>
        <rdfs:label xml:lang="en">datetime set item</rdfs:label>
    </owl:Class>
    


