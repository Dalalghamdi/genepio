    <!-- http://purl.obolibrary.org/obo/IRIDA/IFM#IFM_0000016 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/IRIDA/IFM#IFM_0000016">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/IRIDA/IFM#IFM_0000015"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://purl.obolibrary.org/obo/GENEPIO_0001605"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onDataRange rdf:resource="http://www.w3.org/2001/XMLSchema#decimal"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <IFM:regExValidate rdf:datatype="http://www.w3.org/2001/XMLSchema#string">^([+-]?[1-9]\d*|0)(\.\d+)?$</IFM:regExValidate>
        <rdfs:label xml:lang="en">decimal</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/IRIDA/IFM#IFM_0000017 -->

