        <obo:IAO_0000119>WEB: http://www.google.ca/url?sa=t&amp;source=web&amp;ct=res&amp;cd=1&amp;ved=0CA4QFjAA&amp;url=http%3A%2F%2Fwww.irrd.ca%2Fethics%2Fimages%2FAdverse-event-definition.doc&amp;ei=eqFMS-yFNZSoswOt6siKAQ&amp;usg=AFQjCNGnQcwI8OXH7dqxwo4RgZEEn8X6lg&amp;sig2=UNngkCdO5ZTFvbIB1pKuaA</obo:IAO_0000119>
        <obo:IAO_0000119>WEB: http://en.wikipedia.org/wiki/Serious_adverse_event</obo:IAO_0000119>
        <obo:IAO_0000412 rdf:resource="http://purl.obolibrary.org/obo/oae.owl"/>
    </owl:Class>
    


